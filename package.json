{
    "name": "kalang",
    "version": "0.0.21",
    "description": "A programming language designed to make game development fast and fun!",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "kalang": "./dist/cli.js"
    },
    "scripts": {
        "build": "npx tsup"
    },
    "files": [
        "dist/**/*"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./lexer": {
            "types": "./dist/lexer.d.ts",
            "import": "./dist/lexer.js"
        },
        "./parser": {
            "types": "./dist/parser.d.ts",
            "import": "./dist/parser.js"
        },
        "./transpiler": {
            "types": "./dist/transpiler.d.ts",
            "import": "./dist/transpiler.js"
        },
        "./visitor": {
            "types": "./dist/visitor.d.ts",
            "import": "./dist/visitor.js"
        },
        "./error": {
            "types": "./dist/error.d.ts",
            "import": "./dist/error.js"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "tsup": "^8.0.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@types/node": "^20.11.5",
        "commander": "^11.1.0",
        "glob": "^10.3.10"
    }
}
